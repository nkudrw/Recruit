<%= form_for(@skill) do |f| %>
  <%=  f.text_field :name %>
  <%= f.fields_for :points do |c| %>
  <%= c.hidden_field :skill_id, :value => @skill_last.id+1 %>
  <%= c.hidden_field :user_id, :value => current_user.id %>
  <% end %>
  <%= f.submit '追加', class: 'btn btn-primary btn-block' %>
<% end %>
